<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony-project.org/2.0/container">

	<imports> 
		<import resource="../../apps/Admin/build/di/development_container.xml"/> 
	</imports>
	

	<parameters>
		<parameter key="project.path">C:/Development/Workspace/OxyBase/project/</parameter>
		<parameter key="environment">development</parameter>
	</parameters>


	<services>
		<service id="OxyApplication" class="Oxy_Application" shared="true">
			<file>%project.path%library/Oxy/Application.php</file>
			<argument>%environment%</argument>
			<argument>%project.path%config/config.xml</argument>
			<call method="bootstrap" />
		</service>
	</services>
</container>